{
    "name": "richcongress/normalizer-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "This bundle provides some handy functions and configuration around the Symfony Serializer.",
    "authors": [
        {
            "name": "RichCongress",
            "homepage": "https://www.richcongress.com/"
        }
    ],
    "require": {
        "php": ">=7.1",
        "doctrine/annotations": "^1.7 || ^1.8",
        "symfony/config": "^3.0 || ^4.0",
        "symfony/dependency-injection": "^3.0 || ^4.0",
        "symfony/http-kernel": "^3.0 || ^4.0",
        "symfony/serializer": "^3.0 || ^4.0"
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^2.1",
        "richcongress/unit-bundle": "^1.0",
        "symfony/property-access": "^3.0 || ^4.0",
        "symfony/yaml": "^3.0 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "RichCongress\\NormalizerBundle\\": ""
        },
        "files": [
            "./Serializer/Annotation/VirtualProperty.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\RichCongress\\NormalizerBundle\\": "Tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
